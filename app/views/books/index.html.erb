<h1>Books</h1>

  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Body</th>
      </tr>
    </thead>
    <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= image_tag 'default-image.jpg' %></td>
        <td><%= book.title %></td>
        <td><%= book.body %></td>
      </tr>
<% end %>
    </tbody>
  </table>